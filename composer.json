{
  "name": "the-msy/laravel-passport-one-time-token",
  "description": "Gives the ability to create one time tokens easily with Laravel Passport. Based on lukepolo/laravel-passport-one-time-token package, just updating passport version.",
  "keywords": [
    "laravel",
    "passport",
    "one time token"
  ],
  "homepage": "https://github.com/lukepolo/laravel-passport-one-time-token",
  "license": "MIT",
  "authors": [
    {
      "name": "Luke Policinski",
      "email": "Luke@LukePOLO.com",
      "homepage": "http://LukePOLO.com"
    }
  ],
  "require": {
    "php": ">=5.6",
    "Laravel/Passport": "~4.0"
  },
  "require-dev": {
    "orchestra/testbench": "~3.0",
    "phpunit/phpunit": "~6.0"
  },
  "autoload": {
    "psr-4": {
      "LukePOLO\\LaravelPassportOneTimeToken\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "LukePOLO\\LaravelPassportOneTimeToken\\Tests\\": "tests"
    }
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "LukePOLO\\LaravelPassportOneTimeToken\\ServiceProvider"
      ]
    }
  },
  "prefer-stable" : true,
  "minimum-stability": "dev"
}